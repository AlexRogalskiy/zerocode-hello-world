{
    "scenarioName": "Assert date field format and not its content etc",
    "steps": [
        {
            "name": "get_user_details_incl_dob",
            "url": "/api/v1/google-uk/employees/UK-LON-1002",
            "operation": "GET",
            "request": {
            },
            "assertions": {
                "status": 200,
                "body": {
                    "dob": "$MATCHES.STRING:\\d{4}-\\d{2}-\\d{2}"
                }
            }
        },
        {
            "name": "get_user_any_city_in_uk",
            "url": "/api/v1/google-uk/employees/UK-LON-1002",
            "operation": "GET",
            "request": {
            },
            "assertions": {
                "status": 200,
                "body": {
                    "localtion": "$MATCHES.STRING:UK-(.*)"
                }
            }
        }

    ]
}
